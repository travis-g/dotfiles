#include "/home/t/.colors/base16-eighties.dark"

! #########################################

! Font settings

!URxvt*font: -*-neep-medium-r-semicondensed-*-11-*-*-*-*-*-*-*
!URxvt*boldFont: -*-neep-medium-r-semicondensed-*-11-*-*-*-*-*-*-*
!URxvt*italicFont: -*-neep-medium-r-semicondensed-*-11-*-*-*-*-*-*-*
!URxvt*bolditalicFont:-*-neep-medium-r-semicondensed-*-11-*-*-*-*-*-*-*

!URxvt*font: xft:uushi:pixelsize=9:antialias=true:hinting=true
!URxvt*boldFont: xft:uushi:pixelsize=9:antialias=true:hinting=true
!URxvt*italicFont: xft:uushi:pixelsize=9:antialias=true:hinting=true
!URxvt*bolditalicFont: xft:uushi:pixelsize=9:antialias=true:hinting=true

URxvt*font: xft:tewi:pixelsize=9:antialias=true:hinting=true
URxvt*boldFont: xft:tewi:pixelsize=9:antialias=true:hinting=true


URxvt*urgentOnBell: true
URxvt*scrollBar: false
URxvt.savelines: 3000
urxvt*internalBorder: 5

! URL clicking
URxvt.colorURL: #4682B4
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt.url-select.launcher:  firefox

! Font configs
Xft.antialias: true
Xft.autohint: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcdlight
Xft.rgba: rgb
Xft.dpi: 96
