#include "/home/t/.config/theme/base16-google.dark"

urxvt*font: -*-gohufont-medium-r-normal-*-11-*-*-*-*-*-*-*
urxvt*boldFont: -*-gohufont-bold-r-normal-*-11-*-*-*-*-*-*-*

!urxvt*cursorUnderline: 1
urxvt*cursorBlink: true
urxvt*scrollBar: false
urxvt*internalBorder: 3
urxvt*dynamicColors: on
urxvt*urgentOnBell: true
urxvt*savelines: 1000

! URxvt extra (requires urxvt-perls)
urxvt.perl-ext: default,url-select
urxvt.keysym.M-u: perl:url-select:select_next
urxvt.url-select.launcher: /usr/bin/luakit
urxvt.url-select.underline: true

! XFont configs
Xft*antialias: true
Xft*autohint: true
Xft*hinting: true
Xft*hintstyle: hintslight
Xft*lcdfilter: lcdlight
Xft*rgba: rgb
Xft*dpi: 96
